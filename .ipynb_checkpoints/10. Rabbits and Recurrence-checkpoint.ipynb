    "def wabbits(n, k):\n",
    "    adults = 0\n",
    "    children = 1\n",
    "    \n",
    "    count = 1\n",
    "    newadults = 0\n",
    "    while count < n:\n",
    "        newadults = adults + children\n",
    "        children = adults * k\n",
    "        adults = newadults\n",
    "        count = count + 1\n",
    "    print adults+children"
   ]
